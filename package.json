{
  "name": "platinumcms-server-content",
  "version": "1.0.0",
  "scripts": {
    "serve": "nodemon -q src/listen.ts",
    "debug": "ts-node src/listen.ts",
    "initialise": "ts-node src/initialise.ts",
    "initialise_prod": "cross-env NODE_ENV=production ts-node src/initialise.ts",
    "migrator": "ts-node src/migrator.ts",
    "migrator_up": "ts-node src/migrator.ts up",
    "migrator_up_prod": "cross-env NODE_ENV=production ts-node src/migrator.ts up",
    "migrator_down": "ts-node src/migrator.ts down",
    "migrator_down_prod": "cross-env NODE_ENV=production ts-node src/migrator.ts down",
    "seeder": "ts-node src/seeder.ts",
    "seeder_up": "ts-node src/seeder.ts up",
    "seeder_up_prod": "cross-env NODE_ENV=production ts-node src/seeder.ts up",
    "seeder_down": "ts-node src/seeder.ts down",
    "seeder_down_prod": "cross-env NODE_ENV=production ts-node src/seeder.ts down",
    "test": "npm run test:lint && npm run test:coverage",
    "start": "cross-env NODE_ENV=production ts-node src/listen.ts",
    "pretest": "eslint --ignore-path .gitignore .",
    "eslintfix": "eslint --ignore-path .gitignore . --fix",
    "prepare": "husky install",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit"
  },
  "dependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm": "^9.3.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "ts-node": "^10.9.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/koa-bodyparser": "^4.3.10",
    "husky": "^8.0.3"
  }
}
