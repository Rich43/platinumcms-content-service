{
  "name": "platinumcms-server-content",
  "version": "1.0.0",
  "scripts": {
    "serve": "nodemon -q src/listen.ts",
    "debug": "ts-node src/listen.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "test": "npm run test:lint && npm run test:coverage",
    "start": "cross-env NODE_ENV=production ts-node src/listen.ts",
    "pretest": "eslint --ignore-path .gitignore .",
    "eslintfix": "eslint --ignore-path .gitignore . --fix",
    "prepare": "husky install",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "compile": "tsc"
  },
  "dependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.20",
    "npm": "^9.3.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.2",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/koa-bodyparser": "^4.3.10",
    "husky": "^8.0.3"
  }
}
